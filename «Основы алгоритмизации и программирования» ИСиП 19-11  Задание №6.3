using System;
using System.Collections;

namespace arrayList
{
    class Program
    {
        static void Main(string[] args)
        {
            ArrayList list = new ArrayList() { 12, -5, 7, 55, -127, -330, 1, -21, -78, 22};

            int min = (int)list[0],
                max = (int)list[0];

            for (int i = 0; i < list.Count; i++)
            {
                if ((int)list[i] < min) min = (int)list[i];
                if ((int)list[i] > max) max = (int)list[i];
            }
            Console.WriteLine("Минимальный элемент: " + min);
            Console.WriteLine("Максимальный элемент: " + max);

            Console.WriteLine();
        }
    }
}
